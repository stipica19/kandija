      <div class="container gallery-container">
        <% groblje.forEach((groblje,index)=>{%>
        <h1><%= groblje.groblje%></h1>
            <div class="tz-gallery">    
            <div class="row">
                            <% for(var i=0 ; i < groblje.images.length; i++)  {%>
                <div class="col-sm-6 col-md-4">
                
                    <a class="lightbox" href="../views/public/<%= groblje.images[i]%>">
                        <img src="../views/public/<%= groblje.images[i]%>" alt="Park">
                    </a>
                </div>
                <%}%>   
            </div>    
        </div> 
        <h5><%= groblje.description%></h5>
        <%});%>     
