
  <%- include ('partials/header.ejs') %>
  <script src='https://kit.fontawesome.com/a076d05399.js'></script>
    <!-- Page Content -->
    <!-- Banner Starts Here  -->
 
      
        <div class="container">
          <div class="row">
            <div class="col-lg-12">
              <div class="text-content">
                <h2  style="color:#6e0000; padding: 30px;">PREMINULI</h2>
                
              </div>
            </div>
          </div>
        </div>
      
   
    
    <!-- Banner Ends Here -->

    


    <section class="gallery-block cards-gallery">
      <div class="container">
  
        <div class="col-md-12 text-center">  <img src="../views/assets/images/kriz.svg" width="150px"/></div>  <center><h2 style="color:#6e0000; padding: 30px;">PREMINULI </h2></center>
        <div class="sidebar-item search">
          <form action="/preminuli" method="GET" class="form-inline">
            <input type="text" name="search" placeholder="Tra탑i..">                    <input type="submit" onclick="myFunction()" value="Pretra탑i">
          </form>
        </div>
       
      
        <div class="row display-flex">
       
          <% preminuli.forEach((preminuli)=>{ %>
          <div class="col-md-6 col-lg-6 ">
            <div class="card border-0 transform-on-hover "> 
                <div class="col-md-12 ">
                  <img src="<%= preminuli.images %>" class="card-img-top img-fluid popup  " />
                
                </div>
                  
                <div class="card-body">
                    <h6><i style='font-size:24px' class='fas'>&#xf654;</i> <%= preminuli.naslov%><br><%= preminuli.datum.toLocaleDateString() %></h6>
                    <p class="text-muted card-text"></p>

                </div>
            </div>

          </div>
          <% });%>

<div class="modal fade bd-example-modal-lg" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" aria-hidden="true">
  <div class="modal-dialog modal-lg">
    <div class="modal-content">
     <img class="w-100" id="popup-img" src="" alt="">
    </div>
  </div>
</div>
        </div>
      </div>
      <div class="paginator col-lg-12">
        <% if (typeof(totalPages) != "undefined" && totalPages > 0) { %>
        <nav class="mt-4" aria-label="Page navigation">
            <ul class="pagination justify-content-center ">
                <!-- First page button -->
                <% if (Number(current) === 1) { %>
                    <li class="page-item disabled"><a class="page-link">Po훾etna</a></li>
                <% } else { %>
                    <li class="page-item">
                        <a class="page-link" href="/preminuli?page=1<%if(search){%>?search=<%=search%><%}%>">Po훾etna</a>
                    </li>
                <% } %>
                <!-- Previous page button -->
                <% if (Number(current) === 1) { %>
                    <li class="page-item disabled">
                        <a class="page-link"><i class="fa fa-angle-double-left" aria-hidden="true"></i></a>
                    </li>
                <% } else { %>
                    <li class="page-item">
                        <a class="page-link" href="/preminuli?page=<%= Number(current)-1 %><%if(search){%>?search=<%=search%><%}%>">
                            <i class="fa fa-angle-double-left" aria-hidden="true"></i>
                        </a>
                    </li>
                <% } %>
                
                <% var i = (Number(current) > 3 ? Number(current) - 2 : 1) %>
                <% if (i !== 1) { %>
                    <li class="page-item disabled"><a class="page-link">...</a></li>
                <% } %>
                
                <% for (; i <= totalPages && i <= (Number(current) + 2); i++) { %>
                    <% if (i === Number(current)) { %>
                        <li class="page-item active"><a class="page-link"><%= current %></a></li>
                    <% } else { %>
                        <li class="page-item">
                            <a class="page-link" href="/preminuli?page=<%=i%><%if(search){%>?search=<%=search%><%}%>"><%= i %></a>
                        </li>
                    <% } %>
                    
                    <% if (i === (Number(current) + 2) && i < totalPages) { %>
                        <li class="page-item disabled"><a class="page-link">...</a></li>
                    <% } %>
                <% } %>
                <!-- Next page button -->
                <% if (Number(current) === totalPages) { %>
                    <li class="page-item disabled">
                        <a class="page-link"><i class="fa fa-angle-double-right" aria-hidden="true"></i></a>
                    </li>
                <% } else { %>
                    <li class="page-item">
                        <a class="page-link" href="/preminuli?page=<%= Number(current)+1 %><%if(search){%>?search=<%=search%><%}%>">
                            <i class="fa fa-angle-double-right" aria-hidden="true"></i>
                        </a>
                    </li>
                <% } %>
                <!-- Last page button -->
                <% if (Number(current) === totalPages) { %>
                    <li class="page-item disabled"><a class="page-link">Zadnja</a></li>
                <% } else { %>
                    <li class="page-item">
                        <a class="page-link" href="/preminuli?page=<%= totalPages %><%if(search){%>?search=<%=search%><%}%>">Zadnja</a>
                    </li>
                <% } %>
            </ul>
        </nav>
        <% } %>
    </div>
    </section>
    
    
   
  <%- include ('partials/footer.ejs') %>
